<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.web.dao.TMapMapper" >

    <!--新增图片-->
    <insert id="insertTMap" parameterType="com.spring.web.model.request.TMap" >
        insert into t_map (userId,files) values(#{userId},#{files});
    </insert>

    <!--查询图片-->
    <select id="selectByUserId" resultType="com.spring.web.model.request.TMap">
        select * from t_map where userId= #{id};
    </select>

    <!--删除图片-->
    <delete id="dropByUserId" parameterType="Integer">
        delete from t_map where  userId= #{userId};
    </delete>

    <!--updateMap修改图片-->
    <update id="updateMap" parameterType="com.spring.web.model.request.TMap">
        update t_map set files=#{files} where userId=#{userId};
    </update>


</mapper>

